<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Quotation.
    -->
    <changeSet id="20161020110210-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="assortment_id"
                                 baseTableName="quotation"
                                 constraintName="fk_quotation_assortment_id"
                                 referencedColumnNames="id"
                                 referencedTableName="assortment"/>

        <addForeignKeyConstraint baseColumnNames="customer_id"
                                 baseTableName="quotation"
                                 constraintName="fk_quotation_customer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="customer"/>

    </changeSet>
</databaseChangeLog>
